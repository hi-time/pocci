#!/bin/bash
set -eu

BASE_DIR=$(cd $(dirname $0)/..; pwd)
. ${BASE_DIR}/bin/init-env

cd ${BASE_DIR}

set +e
docker run -w /app -v `pwd`:/app --rm -it ototadana/jenkins-slave-iojs \
       iojs -e "require('./bin/js/lib/yaml2env.js')('config/setup.yml')" >${CONFIG_DIR}/.env
RC=$?
cat ${CONFIG_DIR}/.env
exit ${RC}
