cat << EOF
# LDAP
ldap:
  image: osixia/openldap:${VERSION_OPENLDAP}
  env_file:
    - ./.env
  environment:
    - LDAP_TLS=false
    - LDAP_LOG_LEVEL=256
  ports:
    - "${LDAP_PORT}:389"

user:
  image: xpfriend/pocci-account-center:${VERSION_ACCOUNT_CENTER}
  env_file:
    - ./.env
  links:
   - ldap:${LDAP_HOST}
  restart: always
EOF
